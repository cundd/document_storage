{
    "name": "cundd/document-storage",
    "type": "typo3-cms-extension",
    "description": "",
    "authors": [
        {
            "name": "Daniel Corn",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cundd\\DocumentStorage\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cundd\\DocumentStorage\\Tests\\": "Tests"
        }
    },
    "replace": {
        "document_storage": "self.version",
        "typo3-ter/document-storage": "self.version"
    },
    "suggest": {
        "ext-readline": "For better CLI support"
    }
}
