{
    "name": "cundd/document-storage",
    "type": "typo3-cms-extension",
    "description": "",
    "authors": [
        {
            "name": "Daniel Corn",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "typo3/cms-core": "^11.5 || ^12.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpspec/prophecy": "^1.17",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.6.9"
    },
    "autoload": {
        "psr-4": {
            "Cundd\\DocumentStorage\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cundd\\DocumentStorage\\Tests\\": "Tests"
        }
    },
    "replace": {
        "document_storage": "self.version",
        "typo3-ter/document-storage": "self.version"
    },
    "suggest": {
        "ext-readline": "For better CLI support"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "document_storage"
        }
    }
}
